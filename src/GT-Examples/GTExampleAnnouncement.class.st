Class {
	#name : #GTExampleAnnouncement,
	#superclass : #Announcement,
	#instVars : [
		'example'
	],
	#category : #'GT-Examples-Announcements'
}

{ #category : #accessing }
GTExampleAnnouncement >> example [
	^ example
]

{ #category : #accessing }
GTExampleAnnouncement >> example: anObject [
	example := anObject
]
